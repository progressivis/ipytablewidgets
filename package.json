{
  "name": "ipytablewidgets",
  "version": "0.1.0",
  "description": "'A Custom Jupyter Widget Library for Table-alike Data Transfer'",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progressivis/ipytablewidgets.git"
  },
  "author": "",
  "license": "",
  "main": "ipytablewidgets/static/index",
  "bugs": {
    "url": "https://github.com/progressivis/ipytablewidgets/issues"
  },
  "homepage": "https://github.com/progressivis/ipytablewidgets/#readme",
  "scripts": {
    "format": "prettier --write 'src/*.ts'",
    "lint": "prettier --check 'src/*.ts'",
    "build": "webpack --mode production",
    "test": "npm run test:firefox",
    "test:ci": "karma start --browsers=ChromeCI karma.conf.js",
    "test:chrome": "karma start --browsers=Chrome karma.conf.js",
    "test:debug": "karma start --browsers=Chrome --singleRun=false --debug=true karma.conf.js",
    "test:firefox": "karma start --browsers=Firefox karma.conf.js",
    "watch": "webpack --mode development --watch"
  },
  "dependencies": {
    "@jupyter-widgets/base": "4.0.0",
    "lz4js": "^0.2.0",
    "ndarray": "^1.0.19",
    "pako": "^2.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/ndarray": "^1.0.9",
    "@types/pako": "^1.0.1",
    "@types/webpack-env": "*",
    "chai": "^4.3.4",
    "karma": "^6.3.4",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^2.1.1",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-typescript": "^5.5.1",
    "karma-typescript-es6-transform": "^5.5.1",
    "mocha": "^9.0.2",
    "prettier": "^2.3.0",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "private": true
}
