graft ipytablewidgets/static
graft ipytablewidgets/labextension
graft ipytablewidgets/tests
graft js

prune **/js/node_modules

include LICENSE
include setup.py
include setup.cfg
include pyproject.toml

# Patterns to exclude from any directory
global-exclude *~
global-exclude *.pyc
global-exclude *.pyo
global-exclude .git
global-exclude .ipynb_checkpoints
global-exclude *.map